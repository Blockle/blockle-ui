{
  "name": "@blockle/blocks",
  "version": "0.1.0",
  "description": "Blockle building Blocks",
  "scripts": {
    "start": "npm run build:ts -- -w | npm run build:types -- -w",
    "build": "npm run blocks && rm -rf ./dist/ && npm run build:ts && npm run build:types",
    "build:ts": "rollup -c",
    "build:types": "tsc -d --emitDeclarationOnly",
    "test": "jest",
    "lint": "eslint",
    "format": "prettier --write \"**/*\"",
    "blocks": "ts-node-script ./scripts/build-styles.ts",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "main": "dist/blockle-blocks.js",
  "module": "dist/blockle-blocks.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": "git@github.com:Blockle/blockle-blocks.git",
  "author": "Niek Saarberg",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@blockle/blocks-builder": "^1.1.0",
    "@blockle/form": "^1.0.0",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@svgr/rollup": "^5.1.0",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^11.2.5",
    "@types/history": "^4.7.3",
    "@types/jest": "^26.0.21",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "core-js": "^3.9.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.2.3",
    "jest": "^26.6.3",
    "postcss": "^8.2.8",
    "postcss-import": "^14.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^7.0.2",
    "prettier": "^2.0.2",
    "pretty-quick": "^3.1.0",
    "react": "^17.0.1",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "^17.0.1",
    "react-svg-loader": "^3.0.3",
    "rollup": "^2.42.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-scss": "^3.13.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "@blockle/form": "^1.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "dependencies": {},
  "resolutions": {
    "postcss": "^8.2.8"
  }
}
