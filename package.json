{
  "name": "@blockle/ui",
  "version": "0.0.1-alpha.12",
  "description": "Blockle ui",
  "scripts": {
    "start": "npm run build:ts -- -w | npm run build:types -- -w",
    "build": "rm -rf ./dist/ && npm run build:ts && npm run build:types",
    "build:ts": "rollup -c",
    "build:types": "tsc -d --emitDeclarationOnly",
    "test": "jest",
    "lint": "eslint",
    "format": "prettier --write \"**/*\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "main": "dist/blockle-ui.js",
  "module": "dist/blockle-ui.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": "git@github.com:Blockle/blockle-ui.git",
  "author": "Niek Saarberg",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@blockle/form": "^0.0.1-alpha.13",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-info": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@svgr/rollup": "^5.1.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^8.0.4",
    "@types/history": "^4.7.3",
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "history": "^4.10.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react": "^16.12.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "^16.12.0",
    "react-svg-loader": "^3.0.3",
    "rollup": "^1.27.13",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript2": "^0.25.3",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.13.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.3"
  },
  "peerDependencies": {
    "@blockle/form": "^0.0.1-alpha.13",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "dependencies": {}
}
