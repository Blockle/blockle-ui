{
  "name": "@blockle/blocks",
  "version": "0.0.1-alpha.43",
  "description": "Blockle Blocks",
  "scripts": {
    "start": "npm run build:ts -- -w | npm run build:types -- -w",
    "build": "npm run blocks && rm -rf ./dist/ && npm run build:ts && npm run build:types",
    "build:ts": "rollup -c",
    "build:types": "tsc -d --emitDeclarationOnly",
    "test": "jest",
    "lint": "eslint",
    "format": "prettier --write \"**/*\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "blocks": "ts-node-script ./scripts/build-styles.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "main": "dist/blockle-blocks.js",
  "module": "dist/blockle-blocks.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": "git@github.com:Blockle/blockle-blocks.git",
  "author": "Niek Saarberg",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@blockle/blocks-builder": "^0.0.2-alpha.0",
    "@blockle/form": "^1.0.0",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-info": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@svgr/rollup": "^5.1.0",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.2",
    "@types/history": "^4.7.3",
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^3.0.0",
    "history": "^4.10.1",
    "husky": "^4.2.3",
    "jest": "^25.2.4",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "react": "^16.12.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "^16.12.0",
    "react-svg-loader": "^3.0.3",
    "rollup": "^2.3.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript2": "^0.27.0",
    "stylelint": "^13.2.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.13.0",
    "ts-jest": "^25.3.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.7.3"
  },
  "peerDependencies": {
    "@blockle/form": "^1.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "dependencies": {}
}
