{
  "name": "@blockle/ui",
  "version": "0.0.1-alpha.8",
  "description": "Blockle ui",
  "scripts": {
    "start": "npm run build:ts -- -w | npm run build:types -- -w",
    "build": "rm -rf ./dist/ && npm run build:ts && npm run build:types",
    "build:ts": "rollup -c",
    "build:types": "tsc -d --emitDeclarationOnly",
    "test": "jest",
    "lint": "eslint",
    "format": "prettier --write \"**/*\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "main": "dist/blockle-ui.js",
  "module": "dist/blockle-ui.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": "git@github.com:Blockle/blockle-ui.git",
  "author": "Niek Saarberg",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.4",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "history": "^4.9.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "jest-dom": "^4.0.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rollup": "^1.16.7",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-react-svg": "^3.0.3",
    "rollup-plugin-typescript2": "^0.21.2",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "dependencies": {}
}
